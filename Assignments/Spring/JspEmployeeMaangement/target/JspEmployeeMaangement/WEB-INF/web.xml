<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
<context-param>
  <param-name>msg</param-name>
  <param-value>param context</param-value>
  </context-param>
  <servlet>
    <servlet-name>ServletFormEx</servlet-name>
    <servlet-class>com.examples.java.jsp.employeeManagement.EmployeeForm</servlet-class>
   <init-param>
   <param-name>msg1</param-name>
   <param-value>bhavani</param-value>
   </init-param>
   <init-param>
   <param-name>msg2</param-name>
   <param-value>murali</param-value>
   </init-param>
  </servlet>
  
  <servlet>
    <servlet-name>ServletFormEx1</servlet-name>
    <servlet-class>com.examples.java.jsp.employeeManagement.EmployeeRegisterForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddEmployeeServlet</servlet-name>
  	<display-name>AddEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.examples.java.jsp.employeeManagement.AddEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateEmployeeServlet</servlet-name>
  	<display-name>UpdateEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.examples.java.jsp.employeeManagement.UpdateEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewEmployeeServlet</servlet-name>
  	<display-name>ViewEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.examples.java.jsp.employeeManagement.ViewEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListEmployeeServlet</servlet-name>
  	<display-name>ListEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.examples.java.jsp.employeeManagement.ListEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<display-name>DeleteEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.examples.java.jsp.employeeManagement.DeleteEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletFormEx</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletFormEx1</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddEmployeeServlet</servlet-name>
  	<url-pattern>/add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateEmployeeServlet</servlet-name>
  	<url-pattern>/update.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewEmployeeServlet</servlet-name>
  	<url-pattern>/view.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListEmployeeServlet</servlet-name>
  	<url-pattern>/ListEmployeeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<url-pattern>/delete.do</url-pattern>
  </servlet-mapping>
</web-app>
