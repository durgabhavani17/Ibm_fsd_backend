<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  <param-name>msg</param-name>
  <param-value>param context</param-value>
  </context-param>
  <servlet>
    <servlet-name>ServletFormEx</servlet-name>
    <servlet-class>com.examples.java.employeeServlet.EmployeeForm</servlet-class>
   <init-param>
   <param-name>msg1</param-name>
   <param-value>bhavani</param-value>
   </init-param>
   <init-param>
   <param-name>msg2</param-name>
   <param-value>murali</param-value>
   </init-param>
  </servlet>
  
  <servlet>
    <servlet-name>ServletFormEx1</servlet-name>
    <servlet-class>com.examples.java.employeeServlet.EmployeeRegisterForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletFormEx</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletFormEx1</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
</web-app>
